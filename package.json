{
  "name": "example",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.18.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --no-cache  --continue",
    "lint": "turbo run lint",
    "lint:staged": "lint-staged",
    "pushDB": "cd ./apps/server/prisma && pnpx prisma db push && pnpm prisma generate && cd ../../../",
    "pullDB": "cd ./apps/server/prisma && pnpx prisma db pull && pnpm prisma generate && cd ../../../",
    "openDB": "cd ./apps/server/prisma && pnpx prisma studio",
    "prepare": "husky install"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "turbo": "^1.7.3"
  },
  "devDependencies": {
    "eslint-config-base": "workspace:*",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "typescript": "^4.9.4",
    "tsc-files": "^1.1.3"
  },
  "pre-commit": "lint",
  "license": "MIT"
}
