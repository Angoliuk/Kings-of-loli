generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id              Int      @id @default(autoincrement())
  name            String   @unique
  profile_picture String?
  sound           Int      @default(100)
  score           Int      @default(0)
  password        String
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
